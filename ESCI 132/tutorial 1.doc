\pard\plain\ltrpar\ql\s14\itap0\tqc\tx4320\tqc\tx8640{\s14\f0\fs18\lang5129{\*\listtag0}\abinodiroverride\ltrch 30006956}{\s14\f0\fs18\lang5129{\*\listtag0}\par}
